{
    "manifest_version": 2,
  
    "name": "Gowtham",
    "description": "This extension will analyze a page using GTmetrix",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_title": "Have A Nice Day",
     "default_popup": "popup.html"
    },
    "chrome_url_overrides" : {  "newtab": "newtab.html"},
    "permissions": [
     "activeTab"
     ]
  }