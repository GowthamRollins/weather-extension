{
    "manifest_version": 2,
  
    "name": "Weather Predictor",
    "description": "This extension will get you with your current weather in your location",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_title": "Find your locations current weather",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "geolocation",
     "http://*/"
     ]
  }